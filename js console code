var elements = document.querySelectorAll('[aria-label="Remove"]');

// Check if any elements were found
if (elements.length > 0) {
  // Iterate over the NodeList (collection of elements) and click each element
  elements.forEach(function(element) {
    element.click();
    // You can also log each element to the console if needed
    console.log(element);
  });
} else {
  console.log("No elements found with aria-label='Remove'");
}



